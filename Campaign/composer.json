{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../Composer/laravel-sso",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../Composer/laravel-widgets",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../Composer/laravel-helpers",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "~7.1.0",
        "doctrine/dbal": "~2.3",
        "geoip2/geoip2": "~2.0",
        "laravel/framework": "~5.5.0",
        "laravelcollective/html": "~5.5.0",
        "laravel/tinker": "~1.0",
        "yajra/laravel-datatables-oracle": "^8.0",
        "rigor789/airbrake-laravel": "dev-master",
        "guzzlehttp/guzzle": "^6.2",
        "predis/predis": " ~1.0",
        "razorpay/php-bloom-filter": "^0.8",
        "laracasts/flash": "^3.0",
        "league/uri": "^5.0",
        "remp/laravel-sso": "*",
        "remp/laravel-widgets": "*",
        "remp/laravel-helpers": "*",
        "deployer/deployer": "^6.0",
        "deployer/recipes": "^6.0",
        "recca0120/laravel-tracy": "^1.8",
        "php-amqplib/php-amqplib": "^2.7",
        "fico7489/laravel-pivot": "^2.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "friendsofphp/php-cs-fixer": "*",
        "barryvdh/laravel-ide-helper": "^2.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
