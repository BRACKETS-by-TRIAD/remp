{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "arrilot/laravel-widgets": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.3",
        "bower-asset/animate.css": "^3.5",
        "bower-asset/autosize": "^3.0",
        "bower-asset/bootstrap": "^3.3",
        "bower-asset/bootstrap-select": "^1.12",
        "bower-asset/datatables.net": "^2.1",
        "bower-asset/dropzone": "^4.3",
        "bower-asset/farbtastic": "^1.2",
        "bower-asset/google-material-color": "^1.3",
        "bower-asset/jquery": "^3.1",
        "bower-asset/jquery-bootstrap-purr": "^1.1",
        "bower-asset/jquery-placeholder": "^2.3",
        "bower-asset/knockout": "^3.4",
        "bower-asset/malihu-custom-scrollbar-plugin": "^3.1",
        "bower-asset/material-design-iconic-font": "^2.2",
        "bower-asset/prism": "^1.6",
        "bower-asset/sugar": "^1.4",
        "bower-asset/waves": "^0.7.5",
        "doctrine/dbal": "~2.3",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "yajra/laravel-datatables-oracle": "^7.2",
        "rigor789/airbrake-laravel": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "public/assets/vendor",
                "bower-asset-library": "public/assets/vendor"
            }
        }
    }
}
